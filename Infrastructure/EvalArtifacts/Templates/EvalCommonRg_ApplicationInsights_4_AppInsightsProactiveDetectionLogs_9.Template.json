{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "resources": [
    {
      "location": "[parameters('LocationProp')]",
      "name": "[parameters('L2Name')]",
      "properties": {
        "CustomEmails": "[parameters('CustomEmails')]",
        "Enabled": "[parameters('Enabled')]",
        "RuleDefinitions": {
          "Description": "[parameters('Description')]",
          "DisplayName": "[parameters('DisplayName')]",
          "HelpUrl": "[parameters('HelpUrl')]",
          "IsEnabledByDefault": "[parameters('IsEnabledByDefault')]",
          "IsHidden": "[parameters('IsHidden')]",
          "IsInPreview": "[parameters('IsInPreview')]",
          "Name": "[parameters('RuleDefinitionsName')]",
          "SupportsEmailNotifications": "[parameters('SupportsEmailNotifications')]"
        },
        "SendEmailsToSubscriptionOwners": "[parameters('SendEmailsToSubscriptionOwners')]"
      },
      "type": "Microsoft.Insights/components/ProactiveDetectionConfigs",
      "condition": "[parameters('newOrExisting')]",
      "apiVersion": "2018-05-01-preview"
    }
  ],
  "parameters": {
    "LocationProp": {
      "type": "string"
    },
    "L2Name": {
      "type": "string"
    },
    "CustomEmails": {
      "type": "array"
    },
    "Enabled": {
      "type": "bool"
    },
    "Description": {
      "type": "string"
    },
    "DisplayName": {
      "type": "string"
    },
    "HelpUrl": {
      "type": "string"
    },
    "IsEnabledByDefault": {
      "type": "bool"
    },
    "IsHidden": {
      "type": "bool"
    },
    "IsInPreview": {
      "type": "bool"
    },
    "RuleDefinitionsName": {
      "type": "string"
    },
    "SupportsEmailNotifications": {
      "type": "bool"
    },
    "SendEmailsToSubscriptionOwners": {
      "type": "bool"
    },
    "newOrExisting": {
      "type": "bool"
    }
  }
}