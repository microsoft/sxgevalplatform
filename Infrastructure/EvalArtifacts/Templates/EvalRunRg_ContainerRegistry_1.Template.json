{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "resources": [
    {
      "location": "[parameters('LocationProp')]",
      "name": "[parameters('Name')]",
      "properties": {
        "adminUserEnabled": "[parameters('AdminUserEnabled')]",
        "anonymousPullEnabled": "[parameters('AnonymousPullEnabled')]",
        "autoGeneratedDomainNameLabelScope": "[parameters('AutoGeneratedDomainNameLabelScope')]",
        "dataEndpointEnabled": "[parameters('DataEndpointEnabled')]",
        "encryption": "[parameters('Encryption')]",
        "metadataSearch": "[parameters('MetadataSearch')]",
        "networkRuleBypassOptions": "[parameters('NetworkRuleBypassOptions')]",
        "policies": "[parameters('Policies')]",
        "publicNetworkAccess": "[parameters('PublicNetworkAccess')]",
        "zoneRedundancy": "[parameters('ZoneRedundancy')]"
      },
      "sku": "[parameters('SkuName')]",
      "type": "Microsoft.ContainerRegistry/registries",
      "condition": "[parameters('newOrExisting')]",
      "apiVersion": "2024-11-01-preview"
    }
  ],
  "parameters": {
    "Name": {
      "type": "string"
    },
    "AdminUserEnabled": {
      "type": "string"
    },
    "Policies": {
      "type": "object"
    },
    "Encryption": {
      "type": "object"
    },
    "DataEndpointEnabled": {
      "type": "string"
    },
    "PublicNetworkAccess": {
      "type": "string"
    },
    "NetworkRuleBypassOptions": {
      "type": "string"
    },
    "ZoneRedundancy": {
      "type": "string"
    },
    "AnonymousPullEnabled": {
      "type": "string"
    },
    "MetadataSearch": {
      "type": "string"
    },
    "AutoGeneratedDomainNameLabelScope": {
      "type": "string"
    },
    "SkuName": {
      "type": "object"
    },
    "LocationProp": {
      "type": "string"
    },
    "newOrExisting": {
      "type": "bool"
    }
  }
}