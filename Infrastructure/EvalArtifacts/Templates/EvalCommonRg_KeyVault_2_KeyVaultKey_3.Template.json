{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "resources": [
    {
      "name": "[parameters('L2Name')]",
      "properties": {
        "attributes": {
          "enabled": "[parameters('Enabled')]",
          "exp": "[parameters('Exp')]",
          "exportable": false,
          "nbf": "[parameters('Nbf')]"
        },
        "keyOps": [
          "encrypt",
          "decrypt",
          "sign",
          "verify",
          "wrapKey",
          "unwrapKey"
        ],
        "keySize": "[parameters('KeySize')]",
        "kty": "[parameters('KeyType')]",
        "rotationPolicy": "[if(empty(parameters('RotationPolicy')), json('null'), json(parameters('RotationPolicy')))]"
      },
      "type": "Microsoft.KeyVault/vaults/keys",
      "condition": "[parameters('newOrExisting')]",
      "apiVersion": "2023-02-01"
    }
  ],
  "parameters": {
    "L2Name": {
      "type": "string"
    },
    "Enabled": {
      "type": "bool"
    },
    "Exp": {
      "type": "int"
    },
    "Nbf": {
      "type": "int"
    },
    "KeySize": {
      "type": "int"
    },
    "KeyType": {
      "type": "string"
    },
    "RotationPolicy": {
      "type": "object"
    },
    "newOrExisting": {
      "type": "bool"
    }
  }
}