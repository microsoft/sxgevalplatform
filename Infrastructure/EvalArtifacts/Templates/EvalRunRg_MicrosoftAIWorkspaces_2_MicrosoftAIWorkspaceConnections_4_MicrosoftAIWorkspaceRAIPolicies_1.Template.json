{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "resources": [
    {
      "name": "[parameters('L3Name')]",
      "properties": {
        "basePolicyName": "[parameters('BasePolicyName')]",
        "completionBlocklists": "[parameters('CompletionBlocklists')]",
        "contentFilters": "[parameters('ContentFilters')]",
        "mode": "[parameters('Mode')]",
        "promptBlocklists": "[parameters('PromptBlocklists')]",
        "type": "[parameters('Type')]"
      },
      "type": "Microsoft.MachineLearningServices/workspaces/connections/raiPolicies",
      "condition": "[parameters('newOrExisting')]",
      "apiVersion": "2024-04-01-preview"
    }
  ],
  "parameters": {
    "L3Name": {
      "type": "string"
    },
    "ContentFilters": {
      "type": "array"
    },
    "BasePolicyName": {
      "type": "string"
    },
    "Mode": {
      "type": "string"
    },
    "PromptBlocklists": {
      "type": "array"
    },
    "Type": {
      "type": "string"
    },
    "CompletionBlocklists": {
      "type": "array"
    },
    "newOrExisting": {
      "type": "bool"
    }
  }
}