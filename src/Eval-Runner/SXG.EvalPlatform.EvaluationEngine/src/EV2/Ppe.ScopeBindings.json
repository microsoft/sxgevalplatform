{
  "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/scopeBindings.json",
  "contentVersion": "0.0.0.1",
  "scopeBindings": [
    {
      "scopeTagName": "ContainerAppTag",
      "bindings": [
        {
          "find": "__Region__",
          "replaceWith": "$config(dcmtRegionId)"
        },
        {
          "find": "__Environment__",
          "replaceWith": "test"
        },
        {
          "find": "__Location__",
          "replaceWith": "$location()"
        },
        {
          "find": "__SubscriptionId__",
          "replaceWith": "$azureSubscriptionId()"
        },
        {
          "find": "__RegistryUrl__",
          "replaceWith": "evalplatformregistrytest.azurecr.io"
        },
        {
          "find": "__CommonRg__",
          "replaceWith": "CommonRg-UsEast2Delete"
        },
        {
          "find": "__RunRg__",
          "replaceWith": "RunRg-$config(dcmtRegionId)Delete"
        },
        {
          "find": "__InstrumentationKey__",
          "replaceWith": "InstrumentationKey=31e6e499-df41-4492-b5af-a7b87cfef880;IngestionEndpoint=https://eastus2-3.in.applicationinsights.azure.com/;LiveEndpoint=https://eastus2.livediagnostics.monitor.azure.com/;ApplicationId=61098f4b-1460-4e37-8fc5-db0f976679c1"
        }
      ]
    }
  ]
}