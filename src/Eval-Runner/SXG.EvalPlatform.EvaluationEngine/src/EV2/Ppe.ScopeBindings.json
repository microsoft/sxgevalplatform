{
  "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/scopeBindings.json",
  "contentVersion": "0.0.0.1",
  "scopeBindings": [
    {
      "scopeTagName": "ContainerAppTag",
      "bindings": [
        {
          "find": "__Region__",
          "replaceWith": "$config(dcmtRegionId)"
        },
        {
          "find": "__Environment__",
          "replaceWith": "ppe"
        },
        {
          "find": "__Location__",
          "replaceWith": "$location()"
        },
        {
          "find": "__SubscriptionId__",
          "replaceWith": "$azureSubscriptionId()"
        },
        {
          "find": "__RegistryUrl__",
          "replaceWith": "evalcontainerregistryppe.azurecr.io"
        },
        {
          "find": "__CommonRg__",
          "replaceWith": "ppe-EvalCommon-UsEast2Rg"
        },
        {
          "find": "__RunRg__",
          "replaceWith": "ppe-EvalRunnerRg-$config(dcmtRegionId)Rg"
        },
        {
          "find": "__InstrumentationKey__",
          "replaceWith": "InstrumentationKey=83156844-e927-48e7-a355-b7e51b1aa199;IngestionEndpoint=https://eastus2-3.in.applicationinsights.azure.com/;LiveEndpoint=https://eastus2.livediagnostics.monitor.azure.com/;ApplicationId=1b944d8e-9a4e-4cd9-942d-1e8281e7d624"
        }
      ]
    }
  ]
}