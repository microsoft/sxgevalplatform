{
  "AzureStorage": {
    "AccountName": "sxgevalstoragedev",
    "QueueName": "eval-requests",
    "BlobContainerPrefix": "agent-",
    "UseManagedIdentity": true,
    "ConnectionString": null
  },
  "ApiEndpoints": {
    "EvalConfigEndpoint": "https://sxg-eval-api-dev.azurewebsites.net/api/eval/{evalRunId}/config",
    "StatusUpdateEndpoint": "https://sxg-eval-api-dev.azurewebsites.net/api/eval/{evalRunId}/status"
  },
  "Evaluation": {
    "MaxParallelMetrics": 3,
    "TimeoutSeconds": 300,
    "RetryAttempts": 2
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  }
}