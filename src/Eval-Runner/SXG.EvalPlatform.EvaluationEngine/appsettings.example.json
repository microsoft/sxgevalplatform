{
  "AzureStorage": {
    "AccountName": "sxgevalstoragedev",
    "QueueName": "eval-requests",
    "BlobContainerPrefix": "agent-",
    "UseManagedIdentity": true,
    "ConnectionString": null
  },
  "ApiEndpoints": {
    "BaseUrl": "https://sxgevalapippe.azurewebsites.net",
    "EnrichedDatasetEndpoint": "api/v1/eval/artifacts/enriched-dataset/{EvalRunId}",
    "MetricsConfigurationEndpoint": "api/v1/eval/artifacts/metricsconfiguration/{EvalRunId}",
    "UpdateStatusEndpoint": "/api/v1/eval/runs/{evalRunId}/status",
    "PostResultsEndpoint": "/api/v1/eval/runs/{evalRunId}/results"
  },
  "Evaluation": {
    "MaxParallelMetrics": 3,
    "TimeoutSeconds": 300,
    "RetryAttempts": 2
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  }
}