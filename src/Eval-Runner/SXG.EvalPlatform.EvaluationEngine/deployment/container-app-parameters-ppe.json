{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "containerAppName": {
      "value": "eval-framework-app-ppe"
    },
    "containerAppEnvName": {
      "value": "eval-framework-env-ppe"
    },
    "containerRegistryName": {
      "value": "evalplatformregistryppe"
    },
    "containerImage": {
      "value": "evalplatformregistryppe.azurecr.io/eval-platform:latest"
    },
    "appSettings": {
      "value": [
        {
          "name": "RUNTIME_ENVIRONMENT",
          "value": "PPE"
        }
      ]
    },
    "secrets": {
      "value": [
        {
          "name": "azure-client-id",
          "value": ""
        },
        {
          "name": "azure-client-secret",
          "value": ""
        },
        {
          "name": "azure-tenant-id", 
          "value": ""
        }
      ]
    },
    "minReplicas": {
      "value": 1
    },
    "maxReplicas": {
      "value": 10
    },
    "cpu": {
      "value": "1.0"
    },
    "memory": {
      "value": "2.0Gi"
    },
    "storageAccountName": {
      "value": "sxgagentevalppe"
    },
    "queueName": {
      "value": "eval-processing-requests"
    },
    "queueLength": {
      "value": 5
    },
    "enableQueueScaling": {
      "value": true
    },
    "location": {
      "value": "East US"
    },
    "logAnalyticsWorkspaceName": {
      "value": "eval-platform-logs-ppe"
    }
  }
}