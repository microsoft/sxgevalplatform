{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "region": {
      "type": "string"
    },
    "appServiceName": {
      "type": "string"
    },
    "storageAccountName": {
      "type": "string"
    },
    "environment": {
      "type": "string"
    }
  },
  "resources": [],
  "outputs": {
    "appServiceUrl": {
      "type": "string",
      "value": "[concat('https://', parameters('appServiceName'), '.azurewebsites.net')]"
    },
    "region": {
      "type": "string",
      "value": "[parameters('region')]"
    }
  }
}
