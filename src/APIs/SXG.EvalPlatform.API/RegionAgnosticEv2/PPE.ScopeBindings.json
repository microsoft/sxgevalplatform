{
  "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/scopeBindings.json",
  "contentVersion": "0.0.0.1",
  "scopeBindings": [
    {
      "scopeTagName": "AppServiceTag",
      "bindings": [
        {
          "find": "__Region__",
          "replaceWith": "$config(dcmtRegionId)"
        },
        {
          "find": "__Environment__",
          "replaceWith": "test"
        },
        {
          "find": "__EnvironmentName__",
          "replaceWith": "PPE"
        },
        {
          "find": "__Location__",
          "replaceWith": "$location()"
        },
        {
          "find": "__AppInsightsInstrumentationKey__",
          "replaceWith": "210e93f2-b190-4b0f-8811-629e0f207cba"
        },
        {
          "find": "__AppInsightsConnectionString__",
          "replaceWith": "InstrumentationKey=210e93f2-b190-4b0f-8811-629e0f207cba;IngestionEndpoint=https://eastus2-3.in.applicationinsights.azure.com/;LiveEndpoint=https://eastus2.livediagnostics.monitor.azure.com/;ApplicationId=39b4b217-9b83-4b54-91f3-b0ccb8c3b4aa"
        },
        {
          "find": "__CacheRedisEndpoint__",
          "replaceWith": "evalplatformcachetest.redis.cache.windows.net:6380"
        },
        {
          "find": "__CacheRedisInstanceName__",
          "replaceWith": "evalplatformcachetest"
        },
        {
          "find": "__ServiceBusConnection__",
          "replaceWith": "sxgevalplatformsbtest.servicebus.windows.net"
        }
      ]
    }
  ]
}